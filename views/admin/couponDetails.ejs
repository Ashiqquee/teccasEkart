<main id="main" class="main">

<section class="section profile">
     
        <div class="col-xl-8">

          <div class="card">
            <div class="card-body">
              <!-- Bordered Tabs -->

              <div class="tab-content pt-2">

                <div class="tab-pane fade show active profile-overview" id="profile-overview">
                

                  <h5 class="card-title">Coupon Details</h5>

                  <div class="row">
                    <div class="col-lg-3 col-md-4 label ">Coupon Name : </div>
                    <div class="col-lg-9 col-md-8"><%= coupon.couponName %> </div>
                  </div>

                  <div class="row">
                    <div class="col-lg-3 col-md-4 label">Code : </div>
                    <div class="col-lg-9 col-md-8"><%= coupon.couponCode %> </dv>
                  </div>

                  <div class="row">
                    <div class="col-lg-3 col-md-4 label mt-3">Start Date: </div>
                    <div class="col-lg-9 col-md-8 mt-3"><%= coupon.startDate.toDateString() %></div>
                  </div>

                  <div class="row">
                    <div class="col-lg-3 col-md-4 label">End Date : </div>
                    <div class="col-lg-9 col-md-8"><%= coupon.endDate.toDateString() %></div>
                  </div>

                  <div class="row">
                    <div class="col-lg-3 col-md-4 label">Quantity : </div>
                    <div class="col-lg-9 col-md-8"><%= coupon.quantity %></div>
                  </div>

                  
                  
                  <div class="row">
                    <div class="col-lg-3 col-md-4 label">Minimum Amount : </div>
                    <div class="col-lg-9 col-md-8"><%= coupon.minAmount %></div>
                  </div>

                  <div class="row">
                    <div class="col-lg-3 col-md-4 label">Maximum Discount : </div>
                    <div class="col-lg-9 col-md-8"><%= coupon.maxDiscount %></div>
                  </div>

                  <div class="row">
                    <div class="col-lg-3 col-md-4 label">Status : </div>
                    <div class="col-lg-9 col-md-8"><%= coupon.status === 0 ?  "Active" : "Inactive" %></div>
                  </div>

  

                </div>

                

                  

               

              </div><!-- End Bordered Tabs -->

            </div>
          </div>

        </div>
      </div>
    </section>
</main>


    
