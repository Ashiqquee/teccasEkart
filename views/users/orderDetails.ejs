<section class="h-100 h-custom mt-5 " ">
    <div class="container py-5 h-100">
      <div class="row d-flex justify-content-center align-items-center h-100">
        <div class="col-lg-8 col-xl-6">
          <div class="card border-top border-bottom border-3" style="border-color: #ecfdff !important;">
            <div class="card-body p-5">
  
              <p class="lead fw-bold mb-5" style="color:black;">Purchase Reciept</p>
             <div class="row">
                <div class="col mb-3">
                  <p class="small text-muted mb-1">Start Date</p>
                  <p><%=orders.start_date%></p>
                </div>
                <div class="col mb-3">
                  <p class="small text-muted mb-1">Order No.</p>
                  <p><%=orders._id%></p>
                </div>
              </div>
              
  
              <div class="mx-n5 px-5 py-4" style="background-color: #f2f2f2;">
                
                 <div class="row">
                  <div class="col-md-8 col-lg-9">
                    <p class="mb-0">Payment Type</p>
                  </div>
                  <div class="col-md-4 col-lg-3">
                    <p class="mb-0"><%=orders.paymentType %></p>
                  </div>
                </div>
                <div class="row">
                  <div class="col-md-8 col-lg-9">
                    <p class="mb-0">Status</p>
                  </div>
                  <div class="col-md-4 col-lg-3">
                    <p class="mb-0"> <% if (orders.is_delivered === false && orders.is_returned===0) { %>
  <td>
            Shipped
  </td>
<% } else if (orders.is_delivered === true  && orders.is_returned===0) { %>
  <td>
  Delivered
                    </td>
                    <% } else { %>
                     <td>
                    Returned
                    </td>
                    <% } %></p>
                  </div>
                </div>
                <div class="row">
                    <%orders.item.forEach(item => {%>
                        <div class="col-md-8 col-lg-9">
                            <p><%=item.product.productName%> <span class=" ms-5 middle">x <%= item.quantity %></span></p>
                            
                          </div>
                          <div class="col-md-4 col-lg-3">
                            <p>$ <%=item.price*item.quantity%></p>
                          </div>
                    <%})%>
                </div>
                
                <div class="row">
                  <div class="col-md-8 col-lg-9">
                    <p class="mb-0">Total</p>
                  </div>
                  <div class="col-md-4 col-lg-3">
                    <p class="mb-0">$ $<%=parseInt(orders.totalPrice) %></p>
                  </div>
                </div>
              </div>
  
              
  
             
  
  
              <p class="mt-4 pt-2 mb-0">Want any help? <a  href="" style="color: black;">Please contact
                  us</a></p>
  
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>



  
