  <section class="checkout_area padding_top mb-5">
    <div class="container">
      <div class="returning_customer">
        <div class="check_title">
          
        </div>
        
      
      </div>
      <div class="cupon_area">
        <div class="check_title">
          
        </div>
        <input type="text" placeholder="Enter coupon code" />
        <a class="tp_btn" href="#">Apply Coupon</a>
      </div>
      <form action="/paymentPage" method="post">
      <div class="billing_details">
        <div class="row">
            <div class="col-lg-2">

            </div>
                <div class="col-lg-5">
                     <% if(typeof msg !== 'undefined' ){ %>

                    <p
                      style="
                        font-size: small;
                        color: rgb(0, 61, 3);
                        font-family: 'EB Garamond', serif;
                      "
                      class="ms-3"
                    >
                      <%= msg %>
                    </p>

                    <% } %>
                    <h3> <a href="/addAddress">Add New Address +</a> </h3>
                    
<% for (var i = 0; i < userAddress.length; i++) { %>
  <div class="mt-4 border border-primary">
    <label>
      <input type="radio" name="address" value="<%= i %>">
      <div>
        <%= userAddress[i].name %>,<%= userAddress[i].address %>
      </div>
      
      <div>
        <%= userAddress[i].city %>,<%= userAddress[i].zip %>
      </div>
      
      <div>
        <%= userAddress[i].district %>
      </div>
      <div>
        <%= userAddress[i].state %>
      </div>
      <div>
        <%= userAddress[i].mobile %>,<%= userAddress[i].number %>,
      </div>
      <div>
        <a href="/editAddress?index=<%= i %>">Edit</a>
      </div>
      
    </label>
     
  </div>
<% } %>


                </div>

          

            
         
          <div class="col-lg-5">
            <div class="order_box">
              <h2>Your Order</h2>
              <ul class="list">
                <li>
                  <a >Product
                    <span>Total</span>
                  </a>
                </li>
                  <% items.forEach(function(item) { %>
    <li>
      <a>
        <%= item.product.productName %>
        <span class="middle">x <%= item.quantity %></span>
        <span class="last">$<span id="price-<%= item._id %>"><%= (item.price * item.quantity).toFixed(2) %></span>
      </a>
    </li>
  <% }); %>
                
              </ul>
              <ul class="list list_2">
                <li>
                  
                </li>
                <li>
                 
                </li>
                <li>
                  <a href="#">Total
                    <span> <%= totalPrice %></span>
                  </a>
                </li>
              </ul>
             
             
              
              <button type="submit" class="btn_3">Proceed to Payment</button>
            </div>
          </div>
        </div>
      </div>
      </form>
    </div>
  </section>
