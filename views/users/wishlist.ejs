      <section class="cart_area padding_top">
    <div class="container">
      <div class="cart_inner">
        <div class="table-responsive">
          <table class="table">
            <thead>
              <tr>
                <th scope="col">Product</th>
                <th scope="col">Name</th>
                <th scope="col">Price</th>
                <th scope="col">Brand</th>
                <th scope="col">Action</th>
              </tr>
            </thead>
            <tbody>
             
              
          <% items.forEach((item) => { %>
      <tr>
        <td>
          <div class="media">
            <div class="d-flex">
              <img style="height: 100px;width: 100px;" src="/productImages/<%= item.productImages[0] %>" alt="<%= item.productName %>" />

            </div>
            <div class="media-body">
              
            </div>
          </div>
        </td>
        <td>
          <h5 style="font-weight: bolder;"><%= item.productName %></h5>
        </td>
        <td>
             <h5 style="font-weight: bolder;"><%= item.price  %></h5>
          

        </td>
        <td>
          <div class="product_count">
            
            <h5 style="font-weight: bolder;"><%= item.brand.brandName  %></h5>
           
          </div>
        </td>
        <td>
            <button style="background-color: white;" class="remove-item-btn" data-item-id="<%=item._id%>"><a href="/removeFromWishlist?id=<%= item._id %>"><i class="fa-solid fa-trash"></i></a></button>
            <button style="background-color: white;" class="remove-item-btn" data-item-id="<%=item._id%>"><a href="/addToCart?id=<%= item._id %>"><i class="fas fa-cart-plus"></i></a></button>
        </td>
      </tr>
    <% }); %>
    <tr>
