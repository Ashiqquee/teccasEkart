    <!-- banner part start-->



    <!-- product_list start-->
    <section class="product_list section_padding">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-12">
                    <div class="section_tittle text-center">
                        <h2>Latest <a href="/shop"> <span>shop</span></h2></a>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="product_list_slider owl-carousel">
                        <div class="single_product_list_slider">
                          <div class="row align-items-center justify-content-between">
    
    <% product.forEach(function(item) { %>
        <div class="col-lg-3 col-sm-6">
            <div class="single_product_item">
                <img src="/productImages/<%= item.productImages[0] %>" alt="">
                <div class="single_product_text">
                    <h4><%= item.productName %></h4>
                    <h3>$<%= item.price %></h3>
                    <a href="/productShop?id=<%= item._id %>" >View Details</a>
                    <% if (user && user.wishlist.includes(item._id)) { %>
                                            <a href="/removeFromWishlist?id=<%= item._id %>" class="already_wishlist">
                                            <i class="fa-solid fa-heart"></i> 
                                            </a>
                                        <% } else { %>
                                            <a href="/addToWishlist?id=<%= item._id %>" class="like_us">
                                            <i class="ti-heart"></i> 
                                            </a>
                                        <% } %>
                    <a href="/addToCart?id=<%= item._id %>" class="add_cart">+ add to cart</a>
                     
                </div>
            </div>
        </div>
    <% }); %>
</div>

                        </div>
                      
                    </div>
                </div>
            </div>
        </div>
    </section>
