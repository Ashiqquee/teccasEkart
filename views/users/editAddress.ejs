  <div class="d-flex justify-content-center mt-5 mb-5">

  

 <div class="col-lg-8">
            <h3> Enter Adress Details</h3>
            
          

          <div class="card">
            <div class="card-body">
             

              <!-- Floating Labels Form -->
             <form class="row contact_form" action="/editAddress?index=<%= index %>" method="post" novalidate="novalidate">

              <div class="col-md-6 form-group p_star">
                <input type="text" class="form-control" id="name" name="name" value="<%= address.name %>" required placeholder="Enter Name" />
                
              </div>
              
              <div class="col-md-6 form-group p_star">
                <input type="email" class="form-control" id="email" name="email" value="<%= address.email %>"  required placeholder="Enter Email" />
                
              </div>

              <div class="col-md-6 form-group p_star">
                <input type="email" class="form-control" id="mobile" name="mobile" required value="<%= address.mobile%>" placeholder="Enter Mobile"/>
                
              </div>

               <div class="col-md-6 form-group p_star">
                <input type="text" class="form-control" id="city" name="city" required value="<%= address.city %>" placeholder="Enter City"/>
                
              </div>

              <div class=" col-md-12 form-group p_star">
                <input type="text" class="form-control" id="address" value="<%= address.address %>" name="address" placeholder="Street Adress" />
               
              </div>

               <div class="col-md-12 form-group p_star">
                  <input type="text" class="form-control" id="address" name="district" value="<%= address.district%>" placeholder="Enter District" />
                
                   
              </div>
              
              <div class="col-md-12 form-group p_star">
                  <input type="text" class="form-control" id="address" name="state" value="<%= address.state %>" placeholder="Enter State" />
                
                   
              </div>
                 
              

             <div class="col-md-12 form-group p_star">
                <input type="text" class="form-control" id="zip" value="<%= address.zip %>"  name="zip" placeholder="Enter Zip"/>
                
              </div>
              
             
             
              
              
             
           
              
            
              
             
              
               

              
              <div class=" ms-5 col-md-6 form-group p_star">
                <button class="btn btn-success" type="submit" >Update Address</button>
              </div>
               <% if(typeof message !== 'undefined' ){ %>

                    <p
                      style="
                        font-size: small;
                        color: red;
                        font-family: 'Tilt Warp', cursive;
                      "
                      class="ms-3"
                    >
                      <%= message %>
                    </p>

                    <% } %> <% if(typeof msg !== 'undefined' ){ %>

                    <p
                      style="
                        font-size: small;
                        color: rgb(0, 61, 3);
                        font-family: 'EB Garamond', serif;
                      "
                      class="ms-3"
                    >
                      <%= msg %>
                    </p>

                    <% } %>
            </form>

